    <!-- Vanilla Factions Expanded - Classical -->
        <!-- EN: Scorpion Raid -->
        <VFEC_ScorpionRaid.letterLabelEnemy>Invasão</VFEC_ScorpionRaid.letterLabelEnemy>
        <!-- EN: It looks like they want to set up Scorpions nearby to fire on your colonists. You can try to wait them out - or go get them. -->
        <VFEC_ScorpionRaid.arrivalTextEnemy>Parece que eles colocarão escorpiões nas proximidades para atirar em seus colonos. Você pode tentar esperá-los - ou ir atrás deles.</VFEC_ScorpionRaid.arrivalTextEnemy>
        <!-- EN: Friendlies -->
        <VFEC_ScorpionRaid.letterLabelFriendly>Aliados</VFEC_ScorpionRaid.letterLabelFriendly>
        <!-- EN: It looks like they want to use scorpions to help you. -->
        <VFEC_ScorpionRaid.arrivalTextFriendly>Parece que eles utilizarão escorpiões para ajudá-lo.</VFEC_ScorpionRaid.arrivalTextFriendly>

